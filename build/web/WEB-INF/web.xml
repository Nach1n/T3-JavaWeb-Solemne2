<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Controlador</servlet-name>
        <servlet-class>Controlador.Controlador</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Controlador_f</servlet-name>
        <servlet-class>Controlador.Controlador_f</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Controlador_c</servlet-name>
        <servlet-class>Controlador.Controlador_c</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Controlador_empleado</servlet-name>
        <servlet-class>Controlador.Controlador_empleado</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HorasextrasDAO</servlet-name>
        <servlet-class>ModeloDAO.HorasextrasDAO</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Controlador_horasextras</servlet-name>
        <servlet-class>Controlador.Controlador_horasextras</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Controlador</servlet-name>
        <url-pattern>/Controlador</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Controlador_f</servlet-name>
        <url-pattern>/Controlador_f</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Controlador_c</servlet-name>
        <url-pattern>/Controlador_c</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Controlador_empleado</servlet-name>
        <url-pattern>/Controlador_empleado</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HorasextrasDAO</servlet-name>
        <url-pattern>/HorasextrasDAO</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Controlador_horasextras</servlet-name>
        <url-pattern>/Controlador_horasextras</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>/index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
